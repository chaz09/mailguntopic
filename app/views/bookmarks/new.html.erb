<h1>New Topic</h1>
<div class="row">
  <div class="col-md-8">
<%= form_for [@topic, @bookmark] do |f| %>
<div class="form-group">
  <%= f.label :url %>
  <%= f.text_field :url, class: 'form-control', placeholder: " URL title" %>
      </div>
<div class="form-group">


  <%= f.submit "Save" %>
<% end %>
  </div>
</div>
